
<div class="container my-3">
    <table id="user_table" class="table table-striped container">
            <thead>
                <tr>
                   
                    <th>UserName</th>
                    <th>Email</th>
                    <th>Contact</th>                    
                </tr>
            </thead>
            <tbody> 
                {% for data in my_array %}
                <tr>
                    <td>{{ data.name }}</td>
                    <td>{{ data.email }}</td>
                    <td>{{ data.contact }}</td>
                </tr>
                {% endfor %}

            </tbody>
    </table>
</div>

<script src="../js/jqueryValidator.js"></script>
<script src="../js/validation.js"></script>
<link rel="stylesheet" href="../style/style.css">

